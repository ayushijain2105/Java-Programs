package programs;
import java.util.Scanner;
public class Reverse01 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n,r;
		System.out.println("Enter any no.");
		Scanner ref = new Scanner(System.in);
		n = ref.nextInt();
		while(n>0) {
			r = n%10;
			System.out.print(r);
			n=n/10;
		}

	}

}
